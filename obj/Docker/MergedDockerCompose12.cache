services:
  apptodolistdb:
    container_name: app-to-do_list-db
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_SA_PASSWORD: Password12345
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      apptodolist: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
  todolistapp:
    build:
      context: D:\Apps\ToDoListApp
      dockerfile: ToDoListApp/Dockerfile
      args:
        BUILD_CONFIGURATION: Debug
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ToDoListApp
      target: base
    container_name: ToDoListApp_1
    depends_on:
      apptodolistdb:
        condition: service_started
        required: true
    entrypoint:
      - tail
      - -f
      - /dev/null
    environment:
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DB_HOST: apptodolistdb
      DB_NAME: ToDoList
      DB_PASSWORD: Password12345
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: /.nuget/fallbackpackages
    image: todolistapp:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /.nuget/packages --additionalProbingPath /.nuget/fallbackpackages  "/app/bin/Debug/net8.0/ToDoListApp.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      apptodolist: null
    ports:
      - mode: ingress
        target: 8080
        published: "8080"
        protocol: tcp
    tty: true
    volumes:
